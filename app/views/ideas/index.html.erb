<h1>Ideas:</h1>
<% @ideas.each_with_index do |idea, index| %>
  <%= index + 1 %>
  <%= idea.title %></br>
  <%= idea.body %></br>
  <%= idea.quality %></br>
  </br>
<% end %>
<h2>Make a New Idea:</h2>
<%= form_for(@idea) do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %></br>
  <%= f.label :body %>
  <%= f.text_field :body %></br>
  <%= f.submit "Save" %>
<% end %>
